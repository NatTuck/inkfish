<.header>
  Autograding Jobs
</.header>

<.table id="ag_jobs" rows={@ag_jobs} row_click={&JS.navigate(~p"/subs/#{&1.sub_id}")}>
  <:col :let={ag_job} label="Dupkey">{ag_job.dupkey}</:col>
  <:col :let={ag_job} label="Prio">{ag_job.prio}</:col>
  <:col :let={ag_job} label="Started at">{ag_job.started_at}</:col>
</.table>
